                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/web/tengo-ganas-de-leer-que-libro-me-recomiendas" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/web/tengo-ganas-de-leer-que-libro-me-recomiendas" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
