                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/multimedia/un-nuevo-vistazo-al-renovado-arista-transcoder" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/multimedia/un-nuevo-vistazo-al-renovado-arista-transcoder" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
