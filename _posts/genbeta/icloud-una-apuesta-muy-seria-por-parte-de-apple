                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/mac/icloud-una-apuesta-muy-seria-por-parte-de-apple" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/mac/icloud-una-apuesta-muy-seria-por-parte-de-apple" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
