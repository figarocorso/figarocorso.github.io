                    <button class="button button-small">Me gusta</button>
                    <span class="wsl-count">4</span>
                  </div>
                                    <!-- twitter -->
                  <a class="twitter-share-button" href="http://twitter.com/share" data-count="horizontal" data-url="http://www.genbeta.com/p/66498" data-counturl="http://www.genbeta.com/web/las-consecuencias-del-cierre-de-megaupload" data-text="Las consecuencias del cierre de Megaupload" data-via="genbeta" data-lang="es">Tweet</a>
                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/web/las-consecuencias-del-cierre-de-megaupload" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/web/las-consecuencias-del-cierre-de-megaupload" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
                </div>
