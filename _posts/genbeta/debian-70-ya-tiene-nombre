                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/sistemas-operativos/debian-70-ya-tiene-nombre" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/sistemas-operativos/debian-70-ya-tiene-nombre" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
