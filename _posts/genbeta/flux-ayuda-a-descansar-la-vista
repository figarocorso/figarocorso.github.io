                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/herramientas/flux-ayuda-a-descansar-la-vista" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/herramientas/flux-ayuda-a-descansar-la-vista" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
