                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/genbeta/mejor-aplicacion-movil-de-2011-whatsapp" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/genbeta/mejor-aplicacion-movil-de-2011-whatsapp" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
