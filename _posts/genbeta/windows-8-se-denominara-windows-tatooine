                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/actualidad/windows-8-se-denominara-windows-tatooine" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/actualidad/windows-8-se-denominara-windows-tatooine" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
