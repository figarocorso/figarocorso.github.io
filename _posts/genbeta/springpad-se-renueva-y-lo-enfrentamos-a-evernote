                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/herramientas/springpad-se-renueva-y-lo-enfrentamos-a-evernote" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/herramientas/springpad-se-renueva-y-lo-enfrentamos-a-evernote" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
