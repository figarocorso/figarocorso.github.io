                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/web/ya-esta-disponible-la-beta-de-alicees-en-espana" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/web/ya-esta-disponible-la-beta-de-alicees-en-espana" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
