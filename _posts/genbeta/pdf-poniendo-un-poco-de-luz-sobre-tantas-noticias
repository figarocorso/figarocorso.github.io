                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/actualidad/pdf-poniendo-un-poco-de-luz-sobre-tantas-noticias" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/actualidad/pdf-poniendo-un-poco-de-luz-sobre-tantas-noticias" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
