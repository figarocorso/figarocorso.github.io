                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/ofimatica/focus-writer-13-ahora-con-soporte-para-texto-enriquecido" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/ofimatica/focus-writer-13-ahora-con-soporte-para-texto-enriquecido" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
