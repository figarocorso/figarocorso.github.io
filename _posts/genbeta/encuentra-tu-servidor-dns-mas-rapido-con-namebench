                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/herramientas/encuentra-tu-servidor-dns-mas-rapido-con-namebench" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/herramientas/encuentra-tu-servidor-dns-mas-rapido-con-namebench" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
                </div>
