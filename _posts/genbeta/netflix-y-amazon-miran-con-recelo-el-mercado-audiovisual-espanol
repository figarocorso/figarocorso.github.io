                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/multimedia/netflix-y-amazon-miran-con-recelo-el-mercado-audiovisual-espanol" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/multimedia/netflix-y-amazon-miran-con-recelo-el-mercado-audiovisual-espanol" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
