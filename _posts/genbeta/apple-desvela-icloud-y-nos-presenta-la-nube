                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/mac/apple-desvela-icloud-y-nos-presenta-la-nube" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/mac/apple-desvela-icloud-y-nos-presenta-la-nube" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
                </div>
