                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/redes-sociales-y-comunidades/me-registro-en-twitter-y-ahora-que" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
