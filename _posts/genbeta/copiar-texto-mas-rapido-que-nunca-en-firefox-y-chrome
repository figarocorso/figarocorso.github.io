                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/navegadores/copiar-texto-mas-rapido-que-nunca-en-firefox-y-chrome" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/navegadores/copiar-texto-mas-rapido-que-nunca-en-firefox-y-chrome" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
