                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/linux/ahorra-movimientos-de-raton-con-brightside" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/linux/ahorra-movimientos-de-raton-con-brightside" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
