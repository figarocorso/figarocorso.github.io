                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/movil/facebook-para-android-adelanta-a-la-version-para-iphone" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/movil/facebook-para-android-adelanta-a-la-version-para-iphone" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
