                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/a-fondo/estes-donde-estes-tus-tareas-siempre-con-wunderlist" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/a-fondo/estes-donde-estes-tus-tareas-siempre-con-wunderlist" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
