                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/movil/descubrimos-las-novedades-de-evernote-para-ios" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/movil/descubrimos-las-novedades-de-evernote-para-ios" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
