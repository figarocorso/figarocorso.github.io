                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/actualidad/google-demo-slam-la-tecnologia-es-cosa-de-todos" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/actualidad/google-demo-slam-la-tecnologia-es-cosa-de-todos" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
