                                    <!-- twitter -->
                  <a class="twitter-share-button" href="http://twitter.com/share" data-count="horizontal" data-url="http://www.genbeta.com/p/60269" data-counturl="http://www.genbeta.com/actualidad/wordpress-31-ya-esta-listo-para-descargar" data-text="Wordpress 3.1 ya está listo para descargar" data-via="genbeta" data-lang="es">Tweet</a>
                  <!-- g+ -->
                  <div class="g-plusone" data-size="medium" data-href="http://www.genbeta.com/actualidad/wordpress-31-ya-esta-listo-para-descargar" data-callback="gplusClickHandler" ></div>
                  <!-- facebook like -->
                  <div href="http://www.genbeta.com/actualidad/wordpress-31-ya-esta-listo-para-descargar" class="fb-like" data-send="false" data-layout="button_count" data-show-faces="false"></div>
                </div>
              </div>
              <!-- .social-sharing -->
