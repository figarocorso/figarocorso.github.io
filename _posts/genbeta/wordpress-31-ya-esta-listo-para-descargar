<script type='text/javascript'>
  window.onerror = function(message, file, line) {
    var sFormattedMessage = '[' + file + ' (' + line + ')] ' + message;
    _gaq.push(['_trackEvent', 'Errors', 'Browser', sFormattedMessage, null, true]);
  }
    
</script>

</body>
</html>
